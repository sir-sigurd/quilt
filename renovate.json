{
  "extends": [
    "config:recommended",
    ":maintainLockFilesWeekly",
    ":prHourlyLimitNone",
    ":prConcurrentLimitNone",
    ":dependencyDashboardApproval"
  ],
  "enabledManagers": [
    "npm",
    "dockerfile"
  ],
  "rangeStrategy": "bump",
  "labels": [
    "js dependencies"
  ],
  "packageRules": [
    {
      "matchManagers": ["dockerfile"],
      "labels": ["docker"],
      "major": {
        "enabled": false
      },
      "minor": {
        "enabled": false
      }
    }
  ]
}
